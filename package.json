{
  "name": "tlf-webpack-config",
  "version": "2.0.1",
  "description": "自己配置的webpack脚手架",
  "main": "./src/js/index.js",
  "scripts": {
    "build": "webpack  --config webpack.config.js --env production --progress",
    "start": "webpack-dev-server --inline --config webpack.config.js --open --env development --progress --ishot true",
    "watch": "webpack --config webpack.config.js --watch --env development --progress",
    "test": "jest"
  },
  "keywords": [
    "webpack",
    "config",
    "webpack-config",
    "tlf"
  ],
  "author": "tlf",
  "license": "ISC",
  "bin": {
    "twc": "./bin/index.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^5.9.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-webpack": "^0.5.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "vue-jest": "^3.0.1",
    "vue-loader": "^15.4.2",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.1.3",
    "commander": "^2.19.0",
    "datatables": "^1.10.18",
    "icheck": "^1.0.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "popper.js": "^1.14.6",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlff/tlf-webpack-config.git"
  },
  "bugs": {
    "url": "https://github.com/tlff/tlf-webpack-config/issues"
  },
  "homepage": "https://github.com/tlff/tlf-webpack-config#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^vue$": "vue/dist/vue.common.js",
      "^icheck$": "<rootDir>/__mocks__/icheck.js"
    },
    "testEnvironment": "jest-environment-jsdom-global",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)",
      "**/src/test/**/*.js?(x)"
    ]
  }
}
